    "@openzeppelin/contracts-upgradeable": "^4.8.0-rc.1",
    "@types/jest": "^29.2.3",
    "@uniswap/v3-periphery": "^1.4.3",
    "@uniswap/v3-sdk": "^3.9.0",
    "axios": "^0.27.2",
    "chai-arrays": "^2.2.0",
    "dotenv": "^9.0.2",
    "ethers": "^5.7.2",
    "jest": "^29.3.1",
    "json-format": "^1.0.1",
    "jsonschema": "^1.4.1",
    "merkletreejs": "^0.3.9",
    "prettier": "^2.2.1",
    "ts-jest": "^29.0.3",
